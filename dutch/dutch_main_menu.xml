<?xml version="1.0" encoding="UTF-8"?>
<vxml version="2.1">

<!--
    Dutch main menu.
-->
<menu id="main" dtmf="true">
    <prompt>
        <audio src="audio_files/welcome/full_welcome2.mp3"/>
    </prompt>

    <choice dtmf="1" next="BSE/exchanging_seeds.xml">
        <prompt>
            <audio src="audio_files/welcome/zaden_uitwisselen.mp3"/> Zaden uitwisselen
        </prompt>
    </choice>

    <choice dtmf="2" next="BSE/selling_seeds.xml">
        <prompt>
            <audio src="audio_files/welcome/zaden_verkopen.mp3"/> Zaden verkopen
        </prompt>
    </choice>

    <choice dtmf="3" next="BSE/buying_seeds.xml">
        <prompt>
            <audio src="audio_files/welcome/zaden_kopen.mp3"/> Zaden kopen
        </prompt>
    </choice>

    <choice dtmf="4" next="Inventory/inventory_menu.xml">
        <prompt>
            <audio src="audio_files/welcome/zaad_informatie.mp3"/> Zaad informatie
        </prompt>
    </choice>

    <noinput>
        <audio src="audio_files/sorry/niet_begrepen.mp3"/><reprompt/>
    </noinput>

    <nomatch>
        <audio src="audio_files/sorry/geen_opties.mp3"/><reprompt/>
    </nomatch>
</menu>
</vxml>
